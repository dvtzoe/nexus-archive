{
  "extends": ["../../tsconfig.json", "astro/tsconfigs/strict"],
  "include": [".astro/types.d.ts", "**/*"],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@ui/*": ["../../packages/ui/src/*"]
    },
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  }
}
