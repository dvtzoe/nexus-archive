{
  "name": "@nexus-archive/types",
  "type": "module",
  "version": "0.1.0",
  "description": "types for nexus-archive",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf ./dist",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "include": [
    "./src"
  ],
  "files": [
    "./dist"
  ],
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
