{
  "extends": "../../tsconfig.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": ".",
    "paths": {
      "@ui/*": ["./src/*"]
    },
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true
  }
}
