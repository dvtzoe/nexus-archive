{
  "name": "@nexus-archive/ui",
  "type": "module",
  "version": "0.1.0",
  "description": "ui components for nexus-archive",
  "exports": {
    "./components/*": "./src/components/*",
    "./utils/*": "./src/utils/*",
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf ./dist",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "./dist"
  ],
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@nexus-archive/types": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
